{
  "name": "boilerplate_app",
  "version": "1.0.0",
  "description": "Boilerplate for quick react application setup",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/PMcDevitt/boilerplate_app.git"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "standard --env mocha && node_modules/.bin/mocha test/setup.js test/unit/**/*_test.js",
    "dev": "webpack -d --watch",
    "build": "webpack -p",
    "standard": "standard --fix --env mocha"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/public/",
      "/node_modules/",
      "tmp.js"
    ],
    "globals": ["expect"]
  },
  "author": "Patrick McDevitt",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "enzyme": "^2.7.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "sinon": "^1.17.7",
    "standard": "^9.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
